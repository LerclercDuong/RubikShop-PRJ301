u30c.add(u30m);
u30c.remove(u30t);
}
}else if(u30n < 30 && u30h > 80){
u30c.add(u30t);
u30c.remove(u30m);
}else if(u30n > 30){
u30c.add(u30m);
u30c.remove(u30t);
