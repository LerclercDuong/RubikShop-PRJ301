document.getElementById('u30ham').click();
}
u30c.remove(u30m);
if(document.getElementById('u30').getBoundingClientRect().height > 80){
u30c.add(u30t);
}else{
u30c.remove(u30t);
}
}else if(u30w <= u30min[2]){
